{
  "name": "quiz-app",
  "version": "1.0.0",
  "description": "Simple quiz app with questions and answers",
  "main": "index.html",
  "scripts": {
    "start": "npm-run-all --parallel serve babel:watch sass:watch",
    "serve": "serve .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass src/css/styles.scss src/css/styles.css",
    "sass:watch": "npm run sass -- -w",
    "live-server": "live-server",
    "babel": "babel src -d .",
    "babel:watch": "npm run babel -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxbischof/quiz-app.git"
  },
  "keywords": [
    "quiz"
  ],
  "author": "maxbischof",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxbischof/quiz-app/issues"
  },
  "homepage": "https://github.com/maxbischof/quiz-app#readme",
  "dependencies": {
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4"
  }
}
